<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.happyfarm.happyfarm">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AccountActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".HelloActivity" />
        <activity android:name=".LocationConfirmActivity" />
        <activity android:name=".PowerOnGWActivity" />
        <activity android:name=".SetupGWActivity" />
        <activity android:name=".AddDeviceActivity" />
        <activity android:name=".WelcomeActivity" />
        <activity android:name=".FeaturesActivity"></activity>
    </application>

</manifest>